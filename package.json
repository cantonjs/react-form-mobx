{
	"name": "react-form-mobx",
	"version": "0.5.0",
	"description": "Declarative Form components for React, built on top of MobX",
	"repository": "cantonjs/react-form-mobx",
	"scripts": {
		"start": "cross-env REACT_DEMO=true webpack-dev-server",
		"watch": "yarn test --watchAll",
		"test": "cross-env BABEL_ENV=cjs jest",
		"coverage": "yarn test --coverage",
		"precoveralls": "yarn coverage",
		"coveralls": "cat ./coverage/lcov.info | coveralls",
		"format": "prettier-eslint --write \"+(src|test)/**/*.js\"",
		"rimraf": "rimraf ./dist ./lib ./es",
		"mkdir": "mkdirp ./dist ./lib ./es",
		"prebuild": "run-s rimraf mkdir",
		"build:cjs": "cross-env BABEL_ENV=cjs babel src -d lib --ignore __tests__",
		"build:es": "cross-env BABEL_ENV=es babel src -d es --ignore __tests__",
		"build:umd": "cross-env NODE_ENV=production webpack",
		"build:min": "cross-env NODE_ENV=production webpack --env.minify",
		"build": "run-p build:*",
		"preversion": "yarn test && yarn build"
	},
	"files": [
		"README.md",
		"LICENSE.md",
		"lib",
		"es",
		"dist"
	],
	"main": "lib/index.js",
	"module": "es/index.js",
	"jsnext:main": "es/index.js",
	"author": "Cap32",
	"license": "MIT",
	"dependencies": {
		"create-react-context": "^0.2.2",
		"prop-types": "^15.5.7",
		"react-lifecycles-compat": "^3.0.4",
		"warning": "^4.0.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.25.0",
		"babel-eslint": "^7.2.3",
		"babel-jest": "^22.4.1",
		"babel-loader": "^7.1.4",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-system-import-transformer": "^2.4.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.24.1",
		"coveralls": "^3.0.1",
		"cross-env": "^3.1.3",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-config-cantonjs": "^1.0.0",
		"eslint-config-standard": "^11.0.0",
		"eslint-import-resolver-webpack": "^0.8.3",
		"eslint-loader": "^2.0.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jest": "^21.15.1",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.7.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-standard": "^3.1.0",
		"jest": "^22.4.2",
		"luxon": "^1.2.1",
		"mkdirp": "^0.5.1",
		"mobx": "^4.3.0",
		"mobx-react": "^5.1.2",
		"moment": "^2.22.2",
		"npm-run-all": "^4.0.2",
		"prettier-eslint-cli": "^4.7.0",
		"react": "^16.3.2",
		"react-dom": "^16.3.2",
		"react-test-renderer": "^16.3.2",
		"rimraf": "^2.5.4",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.0.10",
		"webpack-dev-server": "^3.1.4",
		"yarn": "^1.6.0"
	},
	"peerDependencies": {
		"mobx": "^3.0.0 || ^4.0.0 || ^5.0.0",
		"mobx-react": "^4.0.0 || ^5.0.0",
		"react": "^15.0.0 || ^16.0.0"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.js"
		],
		"modulePathIgnorePatterns": [
			"node_modules",
			"lib"
		]
	}
}
